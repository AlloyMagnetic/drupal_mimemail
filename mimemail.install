/**
 * Implementation of hook_uninstall()
 */
function mimemail_uninstall() {
  $variables = array(
    'mimemail_alter',
    'mimemail_crlf',
    'mimemail_engine',
    'mimemail_incoming',
    'mimemail_key',
    'mimemail_textonly',
  );
  foreach ($variables as $variable) {
    variable_del($variable);
  }
}

